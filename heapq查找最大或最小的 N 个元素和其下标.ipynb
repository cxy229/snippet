    "import heapq\n",
    "nums = [1,2,3,4,5]\n",
    "print(heapq.nlargest(3, nums)) # Prints [42, 37, 23]\n",
    "print(heapq.nsmallest(3, nums)) # Prints [-4, 1, 2]\n",
    "print(\"下标\")\n",
    "print(heapq.nlargest(3, range(len(nums)), nums.__getitem__))"
